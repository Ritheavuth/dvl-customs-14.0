<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="One2ManySelectable">
		<div>
			<!-- will add button above one2many selection but if you use widget="one2many_selectable" -->
			<t t-if="!widget.get('effective_readonly')">

				<button style="margin-bottom: 1%;display:none;" class="button_set_received_by btn bg-primary-light"
						href="javascript:void(0)">
						Update Recevied By
				</button>

				<button style="margin-bottom: 1%;display:none;" class="button_confirm_expense_lines btn bg-info-light "
						href="javascript:void(0)">
						Confirm
				</button>
                <button style="margin-bottom: 1%;display:none;" class="button_approve_expense_lines btn bg-warning-light"
                        href="javascript:void(0)">
                    	Approve
                </button>
				<button style="margin-bottom: 1%;display:none;" class="btn_confirm_approve_expense_line btn dvl-bg-light-blue-orange"
						href="javascript:void(0)">
						Confirm and Approve
				</button>
                <button style="margin-bottom: 1%;display:none;" class="button_draft_expense_lines btn btn-secondary"
                        href="javascript:void(0)">
                    	Draft
                </button>
				<button style="margin-bottom: 1%;display:none;" class="btn_pay_other_expense_line btn bg-danger-light"
						href="javascript:void(0)">
						Pay
				</button>
				<button style="margin-bottom: 1%;display:none;" class="btn_direct_pay_other_expense_line btn dvl-bg-light-orange"
						href="javascript:void(0)">
						Direct Pay
				</button>
			</t>
			<t t-if="widget.get('effective_readonly')">
				<span class="oe_form_char_content"></span>
			</t>
		</div>
    </t>
